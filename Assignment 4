#For With Else Statement:
numbers = [1, 2, 3, 4, 5]

for num in numbers:
    if num == 6:
        print("Number 6 found!")
        break
else:
    print("Number 6 not found in the list.")


#While With Else Statement:
count = 0

while count < 5:
    print("Count:", count)
    count += 1
    if count == 3:
        break
else:
    print("Loop completed!")


#Break Statement:
for num in range(1, 6):
    if num == 3:
        break
    print(num)

#Continue Statement:
for num in range(1, 6):
    if num == 3:
        continue
    print(num)

#Pass Statement:
for num in range(1, 6):
    if num == 3:
        pass
    print(num)
